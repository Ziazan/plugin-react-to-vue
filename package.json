{
  "name": "plugin-react-to-vue",
	"displayName": "plugin-react-to-vue",
	"description": "a plugin that converts react to vue",
	"version": "0.0.4",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"icon": "images/icon.jpg",
	"activationEvents": [
        "onCommand:plugin-react-to-vue.start"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "plugin-react-to-vue.start",
				"title": "react-to-vue"
			}
		],
		"menus": {
			"explorer/context": [
			  {
				"command": "plugin-react-to-vue.start",
				"group": "navigation",
				"when": "resourceFilename =~ /.jsx|.tsx|.js/" 
			  }
			]
		  }
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"test2": "ts-node ./src/test/index.test.ts"
	},
	"dependencies": {
		"@babel/core": "^7.15.0",
		"@babel/generator": "^7.15.0",
		"@babel/preset-typescript": "^7.17.12",
		"@babel/types": "^7.15.0",
		"lodash": "^4.17.21",
		"prettier": "2.0.0",
		"traverse": "^0.6.6"
	  },
	"devDependencies": {
		"@types/vscode": "^1.67.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-electron": "^2.1.3",
		"@types/babel-generator": "^6.25.4",
		"@types/babel-types": "^7.0.11",
		"@types/babel__core": "^7.1.15",
		"@types/lodash": "^4.14.172",
		"@types/traverse": "^0.6.32",
		"ts-node": "^10.7.0",
		"tslint": "^5.17.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-config-standard": "^8.0.1"
	},
	"publisher":"littlefourzhang",
	"license": "SEE LICENSE IN LICENSE.txt",
	"bugs": {
		"url": "https://github.com/Ziazan/plugin-react-to-vue/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Ziazan/plugin-react-to-vue"
	  }
}
